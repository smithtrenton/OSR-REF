program CompileOSR_REF;
{$DEFINE SMART8}
{$DEFINE OSRREF_DEBUG}
{$I SRL-OSR/SRL.simba}
{$I OSR-REF/OSR-REF.simba}

var
  i: integer;

begin
  SetupSRL;
  SetupOSRRef;
  i := 0;
  repeat
    SmartGetFieldObject(0, hook_client_player, 'player'+tostr(i));
    inc(i);
  until (i >= 10);
  writeln('loadedSmartObjects: ' + tostr(loadedSmartObjects));
  freeObject(0);
  writeln('loadedSmartObjects: ' + tostr(loadedSmartObjects));
  freeObject(9);
  writeln('loadedSmartObjects: ' + tostr(loadedSmartObjects));
  releaseObject('player2');
  writeln('loadedSmartObjects: ' + tostr(loadedSmartObjects));
  releaseObject('player2');
  writeln('loadedSmartObjects: ' + tostr(loadedSmartObjects));
  releaseObject(9001);
  writeln('loadedSmartObjects: ' + tostr(loadedSmartObjects));
end.

